{
    "name": "phplrt/parser-contracts",
    "type": "library",
    "description": "A set of phplrt parser abstractions",
    "license": "MIT",
    "keywords": ["abstractions", "contracts", "decoupling", "interfaces", "interoperability", "standards", "parser", "syntax", "analysis", "language", "php"],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/parser-contracts"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "phplrt/source-contracts": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Contracts\\Parser\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6|^10.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^1.11",
        "phpstan/phpstan-strict-rules": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Phplrt\\Contracts\\Parser\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev",
            "dev-main": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
