{
    "name": "phplrt/source-contracts",
    "type": "library",
    "description": "A set of phplrt source abstractions",
    "license": "MIT",
    "keywords": ["file", "source", "io", "disk", "read"],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/source-contracts"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Contracts\\Source\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6|^10.0",
        "vimeo/psalm": "^5.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Phplrt\\Contracts\\Source\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev",
            "dev-main": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
