{
    "name": "phplrt/compiler",
    "type": "library",
    "description": "The phplrt compiler package.",
    "license": "MIT",
    "keywords": ["compiler", "builder", "grammar", "language"],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/compiler"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "laminas/laminas-code": "^4.0",
        "phplrt/lexer": "^4.0",
        "phplrt/visitor": "^4.0",
        "phplrt/position": "^4.0",
        "phplrt/parser": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Compiler\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.13"
    },
    "autoload-dev": {
        "psr-4": {
            "Phplrt\\Compiler\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
